$(eval $(call addlib_s,libmycorelib,$(CONFIG_LIBMYCORELIB)))
LIBMYCORELIB_COMMON_INCLUDES-y     += -I$(LIBMYCORELIB_BASE)/include
CINCLUDES-$(CONFIG_LIBMYCORELIB)   += $(LIBMYCORELIB_COMMON_INCLUDES-y)
CXXINCLUDES-$(CONFIG_LIBMYCORELIB) += $(LIBMYCORELIB_COMMON_INCLUDES-y)

LIBMYCORELIB_CFLAGS-$(call gcc_version_ge,8,0) += -Wno-cast-function-type

LIBMYCORELIB_SRCS-y += $(LIBMYCORELIB_BASE)/main.c
LIBMYCORELIB_SRCS-y += $(LIBMYCORELIB_BASE)/section.ld
